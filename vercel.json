{
  "version": 2,
  "builds": [

    {
      "src": "api/astro/[date].js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json|txt|woff2|woff|ttf|map))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
