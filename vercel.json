{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/astro/date.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/astro/([^/]+)", "dest": "/api/astro/date.ts?date=$1" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico|json|txt|woff2?|ttf|map))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
