{
  "version": 2,
  "builds": [
    { "src": "api/astro/date.ts", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "rewrites": [
    { "source": "/api/astro/([^/]+)", "destination": "/api/astro/date.ts?date=$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
